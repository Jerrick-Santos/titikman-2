\n  const userId = props.userId\r\n  console.log(userId)\r\n  const handleClose = () => setShow(false);\r\n  const handleShow = () => setShow(true);\r\n\r\n  const handleSubmit = async (e) => {\r\n\r\n    try {\r\n      const formData = new FormData();\r\n      formData.append('bio', bio);\r\n      formData.append('image', image);\r\n\r\n      // Send the updated data to the backend\r\n      const response = await axios.patch(`http://localhost:4000/api/profile/${userId}`, formData, {\r\n        headers: {\r\n          'Content-Type': 'multipart/form-data',\r\n        },\r\n      });\r\n\r\n      // Handle the response (you can add further logic if needed)\r\n      console.log('Profile updated:', response.data);\r\n\r\n      // Close the modal after successful submission\r\n      handleClose();\r\n      window.location.reload();\r\n    } catch (error) {\r\n      console.error('Error updating profile:', error);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <>\r\n        <button\r\n                    type=\"button\"\r\n                    className=\"btn btn-danger edit-profile-btn\"\r\n                    // data-bs-toggle=\"modal\"\r\n                    // data-bs-target=\"#editprofile\"\r\n                    onClick={handleShow}\r\n                  >\r\n                    Edit Profile\r\n        </button>\r\n\r\n        <Modal show={show} onHide={handleClose}>\r\n        {/* ... (modal content) */}\r\n        <Modal.Body>\r\n          Update User Info\r\n          <form className=\"editmodal\">\r\n            <div className=\"mb-3\">\r\n              <label htmlFor=\"username\" className=\"form-label\">\r\n                Bio\r\n              </label>\r\n              <textarea\r\n                type=\"textarea\"\r\n                value={bio}\r\n                onChange={(e) => setBio(e.target.value)}\r\n                className=\"form-control\"\r\n              />\r\n              <label htmlFor=\"media\" className=\"form-label\">\r\n                Upload Media:\r\n              </label>\r\n              <input\r\n                type=\"file\"\r\n                id=\"media\"\r\n                name=\"media\"\r\n                accept=\"image/*, video/*\"\r\n                className=\"form-control\"\r\n                onChange={(e) => {\r\n                  setImage(e.target.files[0]);\r\n                }}\r\n              />\r\n            </div>\r\n          </form>\r\n        </Modal.Body>\r\n        <Modal.Footer>\r\n          <button onClick={handleClose} className=\"btn btn-light\">\r\n            Close\r\n          </button>\r\n          <button onClick={handleSubmit} className=\"btn btn-primary\">\r\n            Submit\r\n          </button>\r\n        </Modal.Footer>\r\n      </Modal>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default EditUser;","C:\\Users\\jerri\\Documents\\DLSU Acads\\Second Year\\Term-3\\CCAPDEV\\react-test\\frontend\\src\\pages\\RestaurantPage.js",["109","110","111","112","113","114","115","116"],[],"C:\\Users\\jerri\\Documents\\DLSU Acads\\Second Year\\Term-3\\CCAPDEV\\react-test\\frontend\\src\\components\\ReviewCard.js",["117","118","119","120","121","122","123"],[],"C:\\Users\\jerri\\Documents\\DLSU Acads\\Second Year\\Term-3\\CCAPDEV\\react-test\\frontend\\src\\components\\RestaurantProfile.js",[],[],"C:\\Users\\jerri\\Documents\\DLSU Acads\\Second Year\\Term-3\\CCAPDEV\\react-test\\frontend\\src\\components\\EditReviewModal.js",["124"],[],"C:\\Users\\jerri\\Documents\\DLSU Acads\\Second Year\\Term-3\\CCAPDEV\\react-test\\frontend\\src\\components\\AttachmentsModal.js",[],[],"C:\\Users\\jerri\\Documents\\DLSU Acads\\Second Year\\Term-3\\CCAPDEV\\react-test\\frontend\\src\\components\\EditResponseModal.js",["125","126"],[],"C:\\Users\\jerri\\Documents\\DLSU Acads\\Second Year\\Term-3\\CCAPDEV\\react-test\\frontend\\src\\components\\ResponseModal.js",["127","128"],[],{"ruleId":"129","severity":1,"message":"130","line":3,"column":8,"nodeType":"131","messageId":"132","endLine":3,"endColumn":14}